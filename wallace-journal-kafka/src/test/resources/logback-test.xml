<?xml version="1.0"?>
<configuration>
        <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
                <encoder>
                        <pattern>%d{yyyy-MM-dd HH:mm:ss} [%p] [%t] %c %m%n</pattern>
                </encoder>
        </appender>

        <root level="DEBUG">
                <appender-ref ref="STDOUT" />
        </root>
</configuration>